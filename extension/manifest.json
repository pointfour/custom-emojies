{
    "manifest_version": 2,
    "name": "Custom Emojies",
    "version": "0.0.1",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "js": [
                "p.js",
                "whatsapp.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "blobross.png",
        "default_popup": "popup.html",
        "default_title": "Custom Emojies"
    },
    "permissions": [
        "https://firestore.googleapis.com/"
    ]
}